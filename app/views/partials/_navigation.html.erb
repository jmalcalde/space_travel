<header>
	<div class="navbar navbar-default">
		<div class="container-fluid">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#collapibleMenu">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
				<a href="#" class="navbar-brand">Space Travel</a>
			</div>
			<div class="collapse navbar-collapse" id="collapibleMenu">
				<ul class="nav navbar-nav navbar-right">
					<% if user_signed_in? %>
					<li><%= link_to "Logout", destroy_user_session_path, method: :delete %> <%= current_user.email %></li>
					<% else %>
					<li><%= link_to "Sign in", new_user_session_path %></li>
					<li><%= link_to "Sign up", new_user_registration_path %></li>
					<% end %>
				</ul>
			</div>
		</div>
	</div>
</header>