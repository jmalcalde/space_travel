<header>
	<div class="navbar">
		<div class="container-fluid">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#collapibleMenu">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
				
				<a href="<%= root_path %>" class="navbar-brand">Space Travel</a>
				
			</div>
			<div class="collapse navbar-collapse" id="collapibleMenu">
				<ul class="nav navbar-nav navbar-right">
				<li>
				<img src="../cohete.png" alt="Space Travel" height="24" width="24" class="navbar-text" align="left">
				</li>
					<% if user_signed_in? %>
						<p class="navbar-text">Signed in as <%= current_user.name %></p>
						<li><%= link_to "Logout", destroy_user_session_path, method: :delete %> </li>
						
					<% else %>
						<li><%= link_to "Sign in", new_user_session_path %></li>
						<li><%= link_to "Sign up", new_user_registration_path %></li>
					<% end %>
				</ul>
			</div>
		</div>
	</div>
</header>